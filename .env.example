# .env.example

# REQUIRED (per-env)
NEXT_PUBLIC_SUPABASE_URL=https://your-project.supabase.co
NEXT_PUBLIC_SUPABASE_ANON_KEY=eyJhbGciOiJIUzI1NiIsInR5cCI6...
NEXT_PUBLIC_ONB_SUPABASE_SAVE=true

# SERVER-ONLY (never exposed in browser)
# SUPABASE_SERVICE_ROLE_KEY=your_service_role_key_here

# DEV-ONLY convenience (optional)
# NEXT_PUBLIC_DEV_EMAIL=dev.user@example.com
# NEXT_PUBLIC_DEV_PASSWORD=ChangeMe123!

# OpenAI credentials
OPENAI_API_KEY=

# Onboarding V2 feature flags
NEXT_PUBLIC_FEATURE_ONBOARDING_V2_ROUTER=true
NEXT_PUBLIC_DEBUG_UI=false

# Add Another Role feature flag (default OFF)
NEXT_PUBLIC_FEATURE_ONB_ADD_ANOTHER=false